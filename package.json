{
  "name": "disaster-information-client",
  "version": "1.0.0",
  "description": "disaster-informationのクライアント",
  "main": "app.js",
  "scripts": {
    "build": "./node_modules/bower/bin/bower install; ./node_modules/coffee-script/bin/coffee -c gulpfile.coffee; ./node_modules/gulp/bin/gulp.js",
    "test": "npm run test-if-compiled; npm run test-karma",
    "test-if-compiled": "ls ./src/app.js; ls ./src/style.css",
    "test-karma": "node_modules/gulp/bin/gulp.js; ./node_modules/karma/bin/karma start ./spec/karma.conf.coffee --single-run --browsers PhantomJS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c4w/disaster-information-client.git"
  },
  "author": "KamataRyo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c4w/disaster-information-client/issues"
  },
  "homepage": "http://c4w.github.io/disaster-information-client/",
  "devDependencies": {
    "angular-mocks": "^1.5.5",
    "bower": "^1.7.9",
    "coffee-script": "^1.10.0",
    "gulp": "^3.9.1",
    "gulp-coffee": "^2.3.2",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-webserver": "^0.9.1",
    "jasmine": "^2.4.1",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.7",
    "underscore": "^1.8.3"
  }
}
